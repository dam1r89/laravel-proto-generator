    "keywords": ["laravel", "prototyping", "scaffold", "generator", "command"],
    "authors": [{
        "name": "Damir Miladinov",
        "email": "damir89@gmail.com"
    }, {
        "name": "Ivan Mijatovic",
        "email": "ivanmijatovic89@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.4",
        "friendsofphp/php-cs-fixer": "^2.3",
        "mihaeu/html-formatter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "dam1r89\\ProtoGenerator\\": "src/"
        }
    },
    "minimum-stability": "stable"
